{
    "codecaption": {
		"prefix": "codecap",
		"body": [
			"{{< codecaption title=\"${1:title}\" lang=\"${2:language}\" >}}",
			"${3:code}",
			"{{< /codecaption >}}",
			"",
			"$0"
		],
		"description": "codecaption Hugo shortcode"
	},
	"imgcap": {
		"prefix": "imgcap",
		"body": [
			"{{< imgcap title=\"${1:title}\" src=\"${2:filename}.png\" >}}",
			"$0"
		],
		"description": "image caption Hugo shortcode"
	}
}